<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.ecart">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ECart">
        <activity android:name=".finalBill.BillingActivity"></activity>
        <activity android:name=".categoriesItem.CategoriesItemActivity" />
        <activity android:name=".addItem.AddItemActivity" />
        <activity android:name=".updateItem.UpdateActivity" />
        <activity android:name=".categories.CategoryActivity" />
        <activity android:name=".search.SearchActivity" />
        <activity
            android:name=".home.HomeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>